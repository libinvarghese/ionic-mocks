{
  "name": "ionic-mocks-ts",
  "version": "3.0.0",
  "description": "Stubbed and Pre-mocked Ionic2+ Objects",
  "repository": "https://github.com/libinvarghese/ionic-mocks.git",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "author": "Stone C. Lasley",
  "keywords": [
    "ionic",
    "ionic2",
    "ionic3",
    "mocks",
    "mock",
    "doubles"
  ],
  "bugs": {
    "url": "https://github.com/stonelasley/ionic-mocks"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "post-checkout": "scripts/hooks/post-checkout/update-deps.sh",
      "post-merge": "scripts/hooks/post-merge/update-deps.sh",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run precommit"
    }
  },
  "scripts": {
    "test": "jasmine",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc -p tsconfig.mock.json -d",
    "format:fix": "prettier --write './**/*.{js,ts,json,css,scss,html}'",
    "lint:format": "prettier -c './**/*.{js,ts,json,css,scss,html}'",
    "tslint:jasmine": "tslint -c tslint.json -p ./spec/tsconfig.json 'spec/**/*.ts'",
    "tslint:scripts": "tslint -c tslint.json -p ./scripts/tsconfig.json 'scripts/**/*.ts'",
    "tslint:src": "tslint -c tslint.json -p ./tsconfig.json 'src/**/*.ts'",
    "lint:src": "eslint --ext .js,.ts,.json --format=unix .",
    "lint": "run-p lint:*",
    "version": "git add .",
    "prepublish": "npm run build",
    "postversion": "git push && git push --tags && npm publish",
    "release": "semantic-release",
    "checkNotStaged": "ts-node -P scripts/tsconfig.json scripts/check-not-staged.ts",
    "git-commit": "commit",
    "precommit:parallel": "run-p build lint cover",
    "precommit": "run-s checkNotStaged precommit:parallel",
    "cover:check": "nyc --silent npm run test",
    "cover:report": "nyc --check-coverage ${CHECK_COVERAGE:=false} report",
    "cover": "run-s cover:*"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/prompt-cli": "^11.0.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.0",
    "@types/bluebird": "^3.5.33",
    "@types/jasmine": "^3.6.3",
    "@types/jsdom": "^16.2.6",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "bluebird": "^3.7.2",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-no-secrets": "^0.6.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.10.0",
    "husky": "^4.3.8",
    "jasmine": "^3.6.4",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "jsdom": "^16.4.0",
    "lodash": "^4.17.20",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "rxjs-tslint": "^0.1.8",
    "semantic-release": "^17.3.7",
    "simple-git": "^2.31.0",
    "ts-node": "^9.1.1",
    "tslint": "^5.20.1",
    "tslint-ionic-rules": "0.0.21",
    "typescript": "^4.1.3"
  },
  "release": {
    "branch": "master",
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "prepare": [
      {
        "path": "@semantic-release/changelog",
        "changelogFile": "./CHANGELOG.md"
      },
      "@semantic-release/git",
      "@semantic-release/npm"
    ],
    "publish": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "dependencies": {},
  "peerDependencies": {
    "rxjs": "6.x",
    "lodash": "4.x",
    "jasmine-core": "3.x"
  }
}
