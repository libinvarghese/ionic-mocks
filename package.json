{
  "name": "ionic-mocks-ts",
  "version": "3.0.0",
  "description": "Stubbed and Pre-mocked Ionic2+ Objects",
  "repository": "https://github.com/libinvarghese/ionic-mocks.git",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "author": "Stone C. Lasley",
  "keywords": [
    "ionic",
    "ionic2",
    "ionic3",
    "mocks",
    "mock",
    "doubles"
  ],
  "bugs": {
    "url": "https://github.com/stonelasley/ionic-mocks"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "post-checkout": "scripts/hooks/post-checkout/update-deps.sh",
      "post-merge": "scripts/hooks/post-merge/update-deps.sh",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run precommit"
    }
  },
  "scripts": {
    "test": "karma start",
    "build": "rimraf dist && tsc -d",
    "lint:scripts": "tslint -c tslint.json -p ./scripts/tsconfig.json 'scripts/**/*.ts'",
    "lint:src": "tslint -c tslint.json -p ./tsconfig.json 'src/**/*.ts'",
    "lint": "run-p lint:*",
    "version": "git add .",
    "prepublish": "npm run build",
    "postversion": "git push && git push --tags && npm publish",
    "release": "semantic-release",
    "checkNotStaged": "ts-node -P scripts/tsconfig.json scripts/check-not-staged.ts",
    "git-commit": "commit",
    "precommit:parallel": "run-p build lint test",
    "precommit": "run-s checkNotStaged precommit:parallel"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/prompt-cli": "^11.0.0",
    "@semantic-release/changelog": "^2.1.1",
    "@semantic-release/git": "^6.0.1",
    "@semantic-release/github": "^4.2.18",
    "@types/jasmine": "^2.5.47",
    "@types/node": "^8.0.14",
    "commitizen": "^3.0.4",
    "cz-conventional-changelog": "^2.0.0",
    "husky": "^4.3.8",
    "jasmine": "^3.1.0",
    "jasmine-core": "^2.6.3",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coveralls": "^1.2.1",
    "karma-jasmine": "^1.1.2",
    "karma-typescript": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.1",
    "rxjs": "^6.2.2",
    "rxjs-tslint": "^0.1.5",
    "semantic-release": "^15.6.3",
    "simple-git": "^2.31.0",
    "ts-node": "^9.1.1",
    "tslint": "^5.5.0",
    "tslint-ionic-rules": "0.0.11",
    "typescript": "^2.0.9"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branch": "master",
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "prepare": [
      {
        "path": "@semantic-release/changelog",
        "changelogFile": "./CHANGELOG.md"
      },
      "@semantic-release/git",
      "@semantic-release/npm"
    ],
    "publish": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "dependencies": {
    "deprecated-decorator": "^0.1.6"
  }
}
